{
  "name": "ez-world-info",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "fetch-data": "bun run scripts/fetch-data.ts",
    "build:css": "sass src/styles/styles.scss public/styles.css --style=compressed",
    "build:css:watch": "sass src/styles/styles.scss public/styles.css --watch",
    "dev": "bun run fetch-data && bun run build:css && wrangler dev src/index.ts --local",
    "deploy": "bun run fetch-data && bun run build:css && wrangler deploy src/index.ts",
    "test": "bun test"
  },
  "dependencies": {
    "hono": "^4.6.7"
  },
  "devDependencies": {
    "sass": "^1.97.3",
    "typescript": "^5.6.3",
    "wrangler": "4"
  },
  "packageManager": "bun@1.1.26"
}
